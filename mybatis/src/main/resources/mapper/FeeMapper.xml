<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dsy.mybatis.mapper.FeeMapper">

    <select id="selectListByDate" resultType="Fee">
        SELECT * FROM fee t where t.fee_date = #{date} and 1 = #{num}
    </select>
</mapper>